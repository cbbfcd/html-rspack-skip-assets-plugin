    "chai": "4.2.0",
    "css-loader": "3.5.3",
    "html-webpack-plugin": ">=3.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "mocha": "7.1.2",
    "rimraf": "2.6.3",
    "ts-node": "8.9.1",
